{
  "include": [
    "backend"
  ],
  "exclude": [
    "**/__pycache__",
    "**/node_modules",
    "**/venv",
    "**/.venv"
  ],
  "venvPath": "backend",
  "venv": "venv",
  "pythonVersion": "3.12",
  "typeCheckingMode": "basic",
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": false,
  "pythonPlatform": "Linux",
  "executionEnvironments": [
    {
      "root": "backend",
      "pythonVersion": "3.12",
      "pythonPlatform": "Linux",
      "extraPaths": [
        "backend/venv/lib/python3.12/site-packages"
      ]
    }
  ]
}

